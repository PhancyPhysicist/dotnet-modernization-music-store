{
  "app": "dotnet run -p MusicStoreInfra.csproj",
  "context": {
    "DbUsername": "sa",
    "EcrRepoName": "music-store",
    "ContainerImageTag": "latest",
    "ReplicaCount": 2,

    "@aws-cdk/core:enableStackNameDuplicates": "true",
    "aws-cdk:enableDiffNoFail": "true",
    "@aws-cdk/core:stackRelativeExports": "true"
  }
}
